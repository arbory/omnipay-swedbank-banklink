{
    "name": "arbory/omnipay-swedbank-banklink",
    "description": "Swedbank banklink omnipay package",
    "type": "library",
    "authors": [
        {
            "name": "Mārtiņš Ungurs",
            "email": "martins.ungurs@cube.lv"
        }
    ],
    "autoload": {
      "psr-4": { "Omnipay\\SwedbankBanklink\\" : "src/" }
    },
    "require": {
      "omnipay/common": "~3.0",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "require-dev": {
      "omnipay/tests": "~4.0",
      "squizlabs/php_codesniffer": "^3",
      "phpro/grumphp": "^1",
      "php-coveralls/php-coveralls": "^2",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpro/grumphp": true
        }
    }
}
